		// TODO Auto-generated method stub
		return true;
	}

	public boolean run(JSONObject ctxt) {
		logger.info("JSON:" + ctxt.toString());
		return true;
	}

	public void close() {
		// TODO Auto-generated method stub
		
	}

	public String getName() {
		// TODO Auto-generated method stub
		return extractorName;
	}

}
