<syntun id = "dangdang">
    <domain>www.dangdang.com</domain>
    <method>xpath</method>
    <selector id = "a">
        <title>
            <type>text</type>
            <name>title</name>
            <xpath>html head title</xpath>
            <regex></regex>
        </title>
        <price>
            <type>text</type>
            <name>price</name>
            <xpath>html body[class=defwidth] section[class=detail] div[class=left] b span[id=main_price]</xpath>
            <regex></regex>
        </price>
    </selector>
</syntun>